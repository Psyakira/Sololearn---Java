import java.util.Scanner;

public class Program
{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int amount = scanner.nextInt();
        int uanganda=0;
        //your code goes here
        for (int i=0; i<3; i++){
            uanganda = amount/10;
            amount-= uanganda;
        }
        System.out.println(amount);
    }
}
